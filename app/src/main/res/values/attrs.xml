<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountyScrollableTab">
        <attr name="selectedColor" format="color" />
        <attr name="unSelectedColor" format="color" />
        <attr name="tabTextAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="AnimatedImageButton">
        <attr name="initialAvd" format="reference" />
        <attr name="otherAvd" format="reference" />
    </declare-styleable>

    <declare-styleable name="RuledTextInputLayout">
        <attr name="rule">
            <flag name="absent" value="0" />
            <flag name="required" value="1" />
            <flag name="integer" value="2" />
            <flag name="floating" value="4" />
            <flag name="positive" value="8" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LabeledData">
        <attr name="label" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>
</resources>
